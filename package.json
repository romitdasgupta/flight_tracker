{
  "name": "flight-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/select-provider.mjs && concurrently \"npm run dev:server\" \"vite\"",
    "dev:client": "node scripts/select-provider.mjs && vite",
    "dev:server": "tsx watch --env-file=.env.local server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "node dist-server/index.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run && vitest run --config server/vitest.config.ts",
    "test:server": "vitest run --config server/vitest.config.ts",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.21.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^5.0.0",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.0",
    "jsdom": "^24.1.3",
    "msw": "^2.6.6",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^2.1.3"
  }
}
